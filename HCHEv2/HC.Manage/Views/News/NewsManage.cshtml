            //ok: function () {
            //    this.content('hello world!');
            //    return false;
            //},
            url: '/manage/common/transferOrCopyNews/'+markId,
            //quickClose: true,
            onshow: function () {
                //console.log('onshow');
            },
            oniframeload: function () {
                //console.log('oniframeload');
            },
            onclose: function () {
                if (this.returnValue) {
                    transferOrCopy=this.returnValue;
                    copyNewsAjax(transferOrCopy.join(','));
                }
                //console.log('onclose');
            },
            //onremove: function () {
            //    //console.log('onremove');
            //}
        }).showModal();
